<config>
	<config-name>keikai</config-name><!-- used to resolve dependency -->
	<version>
		<version-class>io.keikai.ui.Version</version-class>
		<version-uid>5.0.0.1</version-uid>
		<zk.version>8.0.5</zk.version><!-- or later -->
	</version>
	<listener>
		<listener-class>io.keikai.ui.WebAppInit</listener-class>
	</listener>
	<library-property>
		<name>io.keikai.model.default.Exporter.class</name>
		<value>excel=io.keikai.model.sys.impl.ExcelExporter</value>
	</library-property>
	<library-property>
		<name>io.keikai.model.default.Importer.class</name>
		<value>excel=io.keikai.model.sys.impl.ExcelImporter</value>
	</library-property>
</config>
